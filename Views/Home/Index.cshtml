@model IEnumerable<ApplicationUser>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <table class="table">
        <tr>
            <th>First_Name</th>
            <th>Last_Name</th>
            <th>UserName</th>
            <th>Email</th>
            <th>Action</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
            <td> <label asp-for="@item.FirstName">@item.FirstName</label></td>
            <td> <label asp-for="@item.LastName">@item.LastName</label></td>
            <td> <label asp-for="@item.UserName">@item.UserName</label></td>
            <td> <label asp-for="@item.Email">@item.Email</label></td>
            <td> <a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id">Edit</a> </td>
        </tr>
        }

    </table>
</div>
